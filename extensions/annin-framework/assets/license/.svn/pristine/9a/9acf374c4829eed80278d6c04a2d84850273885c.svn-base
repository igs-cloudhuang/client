/**
 * 判斷是否是GLI環境
 */
export function isGLI(licenseID: LicenseID): boolean{
    return [
        LicenseID.GLI_UKGC,
        LicenseID.GLI_Malta,
        LicenseID.GLI_Colombia,
        LicenseID.GLI_Romania,
        LicenseID.GLI_Italy,
        LicenseID.GLI_Greece,
        LicenseID.GLI_Spain,
        LicenseID.GLI_Switzerland,
        LicenseID.GLI_Sweden,
        LicenseID.GLI_AGCO,
    ].includes(licenseID);
}

/**
 * 判斷是否是GA環境
 */
export function isGA(licenseID: LicenseID): boolean{
    return [
        LicenseID.GA_UKGC,
        LicenseID.GA_Malta,
        LicenseID.GA_Romania,
        LicenseID.GA_AGCO,
        LicenseID.GA_Sweden,
        LicenseID.GA_Switzerland,
        LicenseID.GA_Spain,
        LicenseID.GA_Greece,
        LicenseID.GA_Italy,
    ].includes(licenseID);
}